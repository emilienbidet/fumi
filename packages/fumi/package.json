{
	"name": "fumi",
	"version": "0.0.1",
	"description": "Bun-powered PDF generation server framework built with React and TypeBox.",
	"keywords": [
		"pdf",
		"react",
		"bun",
		"typescript",
		"server",
		"framework",
		"chrome",
		"print"
	],
	"type": "module",
	"module": "src/index.ts",
	"types": "src/index.ts",
	"exports": {
		".": {
			"import": "./src/index.ts",
			"types": "./src/index.ts"
		}
	},
	"scripts": {
		"dev": "bun run --watch src/index.ts"
	},
	"dependencies": {
		"@sinclair/typebox": "^0.34.14",
		"react": "^19.0.0",
		"react-dom": "^19.0.0"
	},
	"devDependencies": {
		"@fumi/typescript-config": "workspace:*",
		"@types/bun": "^1.1.17",
		"@types/react": "^19.0.7",
		"@types/react-dom": "^19.0.3"
	}
}
